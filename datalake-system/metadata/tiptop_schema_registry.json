      "required_fields": [],
      "optional_fields": ["warehouse", "material_category", "part_number"],
      "output_fields": ["img01", "ima02", "img02", "img03", "SUM(img10) as qty"],
      "group_by": ["img01", "ima02", "img02", "img03"],
      "order_by": "qty DESC",
      "examples": [
        "RM05-008 庫存還有多少",
        "查詢 W01 倉庫的庫存",
