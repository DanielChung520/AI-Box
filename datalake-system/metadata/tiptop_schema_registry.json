  "concepts": {
    "MATERIAL_CATEGORY": {
      "description": "物料類別，用於過濾特定類型的物料",
      "mappings": {
        "plastic": {
          "keywords": ["塑料件", "塑膠件", "塑膠", "塑料"],
          "target_field": "ima02",
          "operator": "LIKE",
          "pattern": "%{value}%"
        },
        "metal": {
          "keywords": ["金屬件", "金屬", "鐵件", "鋁件", "鋁", "鋁合金", "鋁合金錠"],
          "target_field": "ima02",
          "operator": "LIKE",
          "pattern": "%{value}%"
        },
        "finished": {
          "keywords": ["成品"],
          "target_field": "ima08",
          "operator": "=",
          "value": "M"
        },
        "raw": {
          "keywords": ["原料"],
          "target_field": "ima08",
          "operator": "=",
          "value": "P"
        },
        "electronic": {
          "keywords": ["電子件", "電子", "電路", "晶片"],
          "target_field": "ima02",
          "operator": "LIKE",
          "pattern": "%{value}%"
        },
        "packaging": {
          "keywords": ["包裝件", "包裝", "紙箱", "塑膠袋"],
          "target_field": "ima02",
          "operator": "LIKE",
          "pattern": "%{value}%"
        }
      }
    },
    "INVENTORY_LOCATION": {
      "description": "庫存位置（倉庫）",
      "mappings": {
        "W01": {
          "keywords": ["原料倉", "原料"],
          "target_field": "img02",
          "operator": "="
        },
        "W02": {
          "keywords": ["成品倉", "成品"],
          "target_field": "img02",
          "operator": "="
        },
        "W03": {
          "keywords": ["半成品倉", "半成品"],
          "target_field": "img02",
          "operator": "="
        },
        "W04": {
          "keywords": ["不良品倉", "不良品"],
          "target_field": "img02",
          "operator": "="
        },
        "W05": {
          "keywords": ["回收倉", "回收"],
          "target_field": "img02",
          "operator": "="
        }
      }
    },
    "TRANSACTION_TYPE": {
      "description": "交易類型編碼",
      "mappings": {
        "101": {
          "keywords": ["採購", "買進", "進貨", "收料", "購買"],
          "target_field": "tlf19",
          "operator": "=",
          "description": "採購進貨"
        },
        "102": {
          "keywords": ["入庫", "完工入庫"],
          "target_field": "tlf19",
          "operator": "=",
          "description": "完工入庫"
        },
        "201": {
          "keywords": ["領料", "生產領料", "領用"],
          "target_field": "tlf19",
          "operator": "=",
          "description": "生產領料"
        },
        "202": {
          "keywords": ["銷售", "賣出", "出貨", "銷售出庫"],
          "target_field": "tlf19",
          "operator": "=",
          "description": "銷售出庫"
        },
        "301": {
          "keywords": ["報廢", "報損", "損耗", "報廢出庫"],
          "target_field": "tlf19",
          "operator": "=",
          "description": "庫存報廢"
        }
      }
    },
    "TIME_EXPRESSION": {
      "description": "時間表達式",
      "mappings": {
        "this_month": {
          "keywords": ["本月", "這個月", "本月分"],
          "sql_expr": "DATE_FORMAT(CURDATE(), '%Y-%m')",
          "field": "tlf06"
        },
        "last_month": {
          "keywords": ["上月", "上個月", "上月分"],
          "sql_expr": "DATE_SUB(CURDATE(), INTERVAL 1 MONTH)",
          "field": "tlf06"
        },
        "this_year": {
          "keywords": ["今年", "本年"],
          "sql_expr": "YEAR(CURDATE())",
          "field": "tlf06"
        },
        "last_year": {
          "keywords": ["去年"],
          "sql_expr": "YEAR(CURDATE()) - 1",
          "field": "tlf06"
        },
        "recent_days": {
          "keywords": ["最近(\\d+)天", "近(\\d+)天", "過去(\\d+)天"],
          "sql_expr": "DATE_SUB(CURDATE(), INTERVAL ${days} DAY)",
          "field": "tlf06",
          "regex_extract": "(\\d+)"
        }
      }
    },
    "BATCH_NO": {
      "description": "批號",
      "mappings": {
        "default": {
          "keywords": ["批號", "batch", "BATCH"],
          "target_field": "img04",
          "operator": "="
        }
      }
    },
    "LOCATION_CODE": {
      "description": "儲位代碼",
      "mappings": {
        "default": {
          "keywords": ["儲位", "位置", "location", "LOC"],
          "target_field": "img03",
          "operator": "="
        }
      }
    }
  },

  "intent_templates": {
    "QUERY_STOCK": {
      "description": "庫存查詢",
      "intent_type": "QUERY",
      "primary_table": "img_file",
      "joins": [
        {
          "table": "ima_file",
          "on": "img01 = ima01",
          "type": "LEFT"
